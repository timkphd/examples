stamp () { date +"%y%m%d%H%M%S"; }

# A simple delta timer (seconds)
# now=`mytime`
# delta=`mytime $now`

mytime () 
{ 
    now=`date +"%s.%N"`;
    if (( $# > 0 )); then
        rtn=$(printf "%0.3f" `echo $now - $1 | bc`);
    else
        rtn=$(printf "%0.3f" `echo $now`);
    fi;
    echo $rtn
}




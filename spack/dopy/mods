#Install useful things in out new versions of python.
#The "use/unuse" and the load python lines will change
#with new builds.

source /nopt/nrel/apps/env.sh

module use /nopt/nrel/apps/pythons/041323_a/tcl/linux-rhel8-icelake
module purge
module load PrgEnv-cray
module load gcc
module load python-3.9.0-gcc-12.1.0-5itvzw5

which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_a/tcl/linux-rhel8-icelake

module use /nopt/nrel/apps/pythons/041323_b/tcl/linux-rhel8-icelake
module purge
module load intel-oneapi-mpi/2021.8.0-intel
module load gcc
module load python-3.9.0-gcc-12.1.0-5itvzw5
which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_b/tcl/linux-rhel8-icelake

module use /nopt/nrel/apps/pythons/041323_c/tcl/linux-rhel8-icelake
module purge
module load openmpi/4.1.5-gcc
module load gcc
module load python-3.9.0-gcc-12.1.0-5itvzw5
which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_c/tcl/linux-rhel8-icelake

module use /nopt/nrel/apps/pythons/041323_d/tcl/linux-rhel8-icelake
module purge
module load mpich/4.1-gcc
module load gcc
module load python-3.9.0-gcc-12.1.0-5itvzw5
which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_d/tcl/linux-rhel8-icelake







#Install useful things in out new versions of python.
#The "use/unuse" and the load python lines will change
#with new builds.

source /nopt/nrel/apps/env.sh

module use /nopt/nrel/apps/pythons/041323_a/tcl/linux-rhel8-icelake
module purge
module load PrgEnv-cray
module load gcc
module load python-3.11.2-gcc-12.1.0-y2z47rv

which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_a/tcl/linux-rhel8-icelake

module use /nopt/nrel/apps/pythons/041323_b/tcl/linux-rhel8-icelake
module purge
module load intel-oneapi-mpi/2021.8.0-intel
module load gcc
module load python-3.11.2-gcc-12.1.0-y2z47rv
which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_b/tcl/linux-rhel8-icelake
module use /nopt/nrel/apps/pythons/041323_c/tcl/linux-rhel8-icelake
module purge
module load openmpi/4.1.5-gcc
module load gcc
module load python-3.11.2-gcc-12.1.0-y2z47rv
which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_c/tcl/linux-rhel8-icelake

module purge
module use /nopt/nrel/apps/pythons/041323_d/tcl/linux-rhel8-icelake
module purge
module load mpich/4.1-gcc
module load gcc
module load python-3.11.2-gcc-12.1.0-y2z47rv
which python
which mpicc
. pipit
pip3 install  --no-cache-dir mpi4py
module purge
module unuse /nopt/nrel/apps/pythons/041323_d/tcl/linux-rhel8-icelake








